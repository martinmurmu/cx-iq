<h1>Listing product_reports</h1>

<table>
  <tr>
    <th>Product category</th>
    <th>Brand</th>
    <th>Sorting field</th>
    <th>Sorting order</th>
    <th>Number of reviews</th>
    <th>Nps range</th>
    <th>User</th>
  </tr>

<% @product_reports.each do |product_report| %>
  <tr>
    <td><%=h product_report.product_category_id %></td>
    <td><%=h product_report.manufacturer_id %></td>
    <td><%=h product_report.sorting_field %></td>
    <td><%=h product_report.sorting_order %></td>
    <td><%=h product_report.number_of_reviews %></td>
    <td><%=h product_report.nps_range %></td>
    <td><%=h product_report.user_id %></td>
    <td><%= link_to 'Show', product_report %></td>
    <td><%= link_to 'Edit', edit_nps_report_path(product_report) %></td>
    <td><%= link_to 'Destroy', product_report, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New product_report', new_product_report_path %>